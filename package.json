{
  "name": "website",
  "packageManager": "yarn@4.9.4",
  "type": "module",
  "scripts": {
    "start": "yarn run dev",
    "dev": "run-p 'dev:*' --print-label",
    "dev:eleventy": "eleventy --serve",
    "dev:vite": "vite",
    "build": "NODE_ENV=production run-s clean build:vite build:eleventy workbox clean:vite --print-label",
    "build:eleventy": "eleventy",
    "build:vite": "vite build",
    "clean": "del-cli dist",
    "clean:vite": "del-cli dist/.vite",
    "workbox": "workbox generateSW workbox.config.cjs",
    "debug": "DEBUG='Eleventy*' yarn start",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-fetch": "^5.1.0",
    "@11ty/eleventy-navigation": "^1.0.4",
    "@11ty/eleventy-plugin-rss": "^2.0.4",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
    "@sindresorhus/slugify": "^3.0.0",
    "del-cli": "^7.0.0",
    "eleventy-plugin-page-assets": "git+https://github.com/maxboeck/eleventy-plugin-page-assets.git",
    "eleventy-plugin-svg-sprite": "^2.4.5",
    "focus-trap": "^7.6.6",
    "gsap": "^3.13.0",
    "html-minifier": "^4.0.0",
    "imagesloaded": "^5.0.0",
    "infinite-scroll": "^5.0.0",
    "jsdom": "^27.2.0",
    "lodash": "^4.17.21",
    "luxon": "^3.7.2",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "photoswipe": "^5.4.4",
    "sanitize-html": "^2.17.0",
    "sass": "^1.93.2",
    "sharp": "^0.34.5",
    "vite": "^7.1.12",
    "workbox-cli": "^7.3.0"
  },
  "resolutions": {
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@11ty/eleventy-img": "^6.0.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "netlify-cli": "^23.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-jinja-template": "^2.1.0"
  }
}
