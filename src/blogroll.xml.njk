---
permalink: /blogroll.xml
excludeFromSitemap: true
---

<?xml version="1.0" encoding="UTF-8"?>
<opml version="1.0">
  <head>
    <title>blogroll RSS feeds</title>
  </head>
  <body>
    {% for item in blogroll %}
      {% if item.feed %}
        <outline
          type="rss"
          title="{{ item.title }}"
          xmlUrl="{{ item.feed }}"
          htmlUrl="{{ item.url }}"
        />
      {% endif %}
    {% endfor %}
  </body>
</opml>
